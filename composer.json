{
	"name": "baraja-core/cms-static-page",
	"description": "Basic static page editor.",
	"homepage": "https://github.com/baraja-core/cms-static-page",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"nette/utils": "^3.0",
		"nette/di": "^3.0",
		"baraja-core/doctrine": "^2.0",
		"baraja-core/localization": "^1.0",
		"baraja-core/structured-api": "^2.0",
		"baraja-core/markdown-latte-filter": "^1.0",
		"baraja-core/plugin-system": "^1.0",
		"baraja-core/selectbox-tree": "^1.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6",
		"symplify/easy-coding-standard": "^7.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
